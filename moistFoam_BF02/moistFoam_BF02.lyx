\begin_layout Standard
Equations used by 
\begin_inset CommandInset citation
LatexCommand citet
key "BF02"

\end_inset

 in Einstein summation notation:
\begin_inset Formula 
\begin{eqnarray}
\frac{\partial u_{i}}{\partial t}= & -\frac{\partial(u_{i}u_{j})}{\partial x_{j}}+u_{i}\frac{\partial u_{j}}{\partial x_{j}} & -c_{p}\theta_{\rho}\frac{\partial\pi^{\prime}}{\partial x_{i}}+\delta_{i3}g\left(\frac{\theta_{\rho}}{\theta_{\rho0}}-1\right)+K_{d}\frac{\partial D}{\partial x_{i}}\\
\frac{\partial\pi}{\partial t}= & -\frac{\partial(u_{j}\pi)}{\partial x_{j}}+\pi\frac{\partial u_{j}}{\partial x_{j}} & -\pi\frac{R}{c_{p}}\frac{c_{pml}}{c_{vml}}\frac{\partial u_{j}}{\partial x_{j}}+\frac{R}{c_{p}}\left(\frac{L_{v}}{c_{vml}\theta}-\pi\frac{R_{v}c_{pml}}{R_{m}c_{vml}}\right)\dot{r}_{cond}\\
\frac{\partial\theta}{\partial t}= & -\frac{\partial(u_{j}\theta)}{\partial x_{j}}+\theta\frac{\partial u_{j}}{\partial x_{j,}} & -\theta\left(\frac{R_{m}}{c_{vml}}-\frac{Rc_{pml}}{c_{p}c_{vml}}\right)\frac{\partial u_{j}}{\partial x_{j}}+\left[\frac{c_{v}L_{v}}{c_{vml}c_{p}\pi}-\theta\frac{R_{v}}{c_{vml}}\left(1-\frac{R}{c_{p}}\frac{c_{pml}}{R_{m}}\right)\right]\dot{r}_{cond}\\
\frac{\partial r_{v}}{\partial t}= & -\frac{\partial(u_{j}r_{v})}{\partial x_{j}}+r_{v}\frac{\partial u_{j}}{\partial x_{j}} & -\dot{r}_{cond}\\
\frac{\partial r_{c}}{\partial t}= & -\frac{\partial(u_{j}r_{c})}{\partial x_{j}}+r_{c}\frac{\partial u_{j}}{\partial x_{j}} & +\dot{r}_{cond}
\end{eqnarray}

\end_inset

Equation of state
\begin_inset Formula 
\begin{equation}
p=\rho_{a}RT(1+r_{v}/\varepsilon)
\end{equation}

\end_inset

or 
\begin_inset Formula 
\begin{equation}
p_{00}\pi^{\frac{1-\kappa}{\kappa}}=\rho_{a}R\theta\left(1+\frac{r_{v}}{\epsilon}\right)
\end{equation}

\end_inset

where 
\begin_inset Formula $\kappa=R/c_{p}$
\end_inset

, 
\begin_inset Formula $\theta=T/\pi$
\end_inset

 and 
\begin_inset Formula $\pi=(p/p_{00})^{\kappa}$
\end_inset

.
\end_layout

\begin_layout Standard
In vector calculus notation
\begin_inset Formula 
\begin{eqnarray}
\frac{\partial\mathbf{u}}{\partial t}+\nabla\cdot\left(\mathbf{u}\mathbf{u}\right)-\mathbf{u}\left(\nabla\cdot\mathbf{u}\right) & = & -c_{p}\theta_{\rho}\nabla\pi^{\prime}+\mathbf{g}\left(\frac{\theta_{\rho}}{\theta_{\rho0}}-1\right)+K_{d}\nabla D\\
\frac{\partial\pi}{\partial t}+\nabla\cdot\left(\pi\mathbf{u}\right)-\pi\left(\nabla\cdot\mathbf{u}\right) & = & -\pi\frac{R}{c_{p}}\frac{c_{pml}}{c_{vml}}\nabla\cdot\mathbf{u}+\frac{R}{c_{p}}\left(\frac{L_{v}}{c_{vml}\theta}-\pi\frac{R_{v}c_{pml}}{R_{m}c_{vml}}\right)\dot{r}_{cond}\\
\frac{\partial\theta}{\partial t}+\nabla\cdot\left(\theta\mathbf{u}\right)-\theta\left(\nabla\cdot\mathbf{u}\right) & = & -\theta\left(\frac{R_{m}}{c_{vml}}-\frac{Rc_{pml}}{c_{p}c_{vml}}\right)\nabla\cdot\mathbf{u}+\left[\frac{c_{v}L_{v}}{c_{vml}c_{p}\pi}-\theta\frac{R_{v}}{c_{vml}}\left(1-\frac{R}{c_{p}}\frac{c_{pml}}{R_{m}}\right)\right]\dot{r}_{cond}\\
\frac{\partial r_{v}}{\partial t}+\nabla\cdot\left(r_{v}\mathbf{u}\right)-r_{v}\left(\nabla\cdot\mathbf{u}\right) & = & -\dot{r}_{cond}\\
\frac{\partial r_{c}}{\partial t}+\nabla\cdot\left(r_{c}\mathbf{u}\right)-r_{c}\left(\nabla\cdot\mathbf{u}\right) & = & \dot{r}_{cond}
\end{eqnarray}

\end_inset


\end_layout

\begin_layout Standard
In flux form, without the hydrostatic reference profile and with the continuity
 equation:
\begin_inset Formula 
\begin{eqnarray}
\frac{\partial\rho_{a}\mathbf{u}}{\partial t}+\nabla\cdot\left(\rho_{a}\mathbf{u}\mathbf{u}\right) & = & -c_{p}\rho_{a}\theta_{\rho}\nabla\pi+\rho_{a}\mathbf{g}+\rho_{a}K_{d}\nabla D\\
\frac{\partial\rho_{a}}{\partial t}+\nabla\cdot(\rho_{a}\mathbf{u}) & = & 0\\
\frac{\partial\rho_{a}\pi}{\partial t}+\nabla\cdot\left(\pi\rho_{a}\mathbf{u}\right) & = & -\rho_{a}\pi\frac{R}{c_{p}}\frac{c_{pml}}{c_{vml}}\nabla\cdot\mathbf{u}+\rho_{a}\frac{R}{c_{p}}\left(\frac{L_{v}}{c_{vml}\theta}-\pi\frac{R_{v}c_{pml}}{R_{m}c_{vml}}\right)\dot{r}_{cond}\\
\frac{\partial\rho_{a}\theta}{\partial t}+\nabla\cdot\left(\theta\rho_{a}\mathbf{u}\right) & = & -\rho_{a}\theta\left(\frac{R_{m}}{c_{vml}}-\frac{Rc_{pml}}{c_{p}c_{vml}}\right)\nabla\cdot\mathbf{u}+\rho_{a}\theta\left[\frac{c_{v}L_{v}}{c_{vml}c_{p}T}-\frac{R_{v}}{c_{vml}}\left(1-\frac{R}{c_{p}}\frac{c_{pml}}{R_{m}}\right)\right]\dot{r}_{cond}\\
\frac{\partial\rho_{a}r_{v}}{\partial t}+\nabla\cdot\left(r_{v}\rho_{a}\mathbf{u}\right) & = & -\rho_{a}\dot{r}_{cond}\\
\frac{\partial\rho_{a}r_{c}}{\partial t}+\nabla\cdot\left(r_{c}\rho_{a}\mathbf{u}\right) & = & \rho_{a}\dot{r}_{cond}
\end{eqnarray}

\end_inset


\end_layout

\begin_layout Standard
The saturation mixing ratio, 
\begin_inset Formula $r_{vs}$
\end_inset

 can be found from the saturation vapour pressure, 
\begin_inset Formula $e_{s}$
\end_inset

:
\begin_inset Formula 
\begin{equation}
r_{vs}=\varepsilon\frac{e_{s}}{p-e_{s}}
\end{equation}

\end_inset

and 
\begin_inset Formula $e_{s}$
\end_inset

 can be found from an approximate integration of the Clausius-Clapyeron
 relation: 
\begin_inset Formula 
\begin{equation}
p_{vs}=p_{vs0}\exp\left(-\frac{L_{v0}}{R_{v}}\left(\frac{1}{T}-\frac{1}{T_{0}}\right)\right)
\end{equation}

\end_inset

where 
\begin_inset Formula $p_{vs0}=611.3\ \text{Pa}$
\end_inset

 is the saturation vapour pressure at 
\begin_inset Formula $T=T_{0}$
\end_inset

.
\end_layout

\begin_layout Standard
The condensation rate is give by:
\begin_inset Formula 
\begin{equation}
(r_{v}-r_{vs})/\left[\Delta t\left(1+\frac{L_{v}^{2}r_{vs}}{c_{p}R_{v}T^{2}}\right)\right]
\end{equation}

\end_inset


\end_layout

