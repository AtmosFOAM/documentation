Relationships between mixing ratios, mass mixing ratios and partial pressures
 (not needed):
\begin_inset Formula 
\begin{align}
q_{v} & =\rho_{v}/\rho & q_{\ell} & =v_{\ell}\rho_{\ell}/\rho & q_{a} & =\rho_{a}/\rho\nonumber \\
r_{v} & =\rho_{v}/\rho_{a} & r_{\ell} & =v_{\ell}\rho_{\ell}/\rho_{a} & r_{t} & =r_{v}+r_{\ell}\\
\rho & =\rho_{a}+\rho_{v}+v_{\ell}\rho_{\ell} & 1 & =q_{a}+q_{v}+q_{\ell} & q_{t} & =q_{v}+q_{\ell}\\
q_{v} & =r_{v}/(1+r_{t}) & q_{\ell} & =r_{\ell}/(1+r_{t}) & q_{t} & =r_{t}/(1+r_{t})\\
r_{v} & =q_{v}/q_{a} & r_{\ell} & =q_{\ell}/q_{a} & q_{a} & =1/(1+r_{t})\\
 &  & 1-q_{\ell} & =(1+r_{v})/(1+r_{t})
\end{align}

\end_inset


\begin_inset Formula 
\begin{eqnarray}
\frac{p_{v}}{p}=\frac{1-v_{\ell}}{1+\varepsilon/r_{v}} &  & \frac{p_{a}}{p}=\frac{1-v_{\ell}}{1+r_{v}/\varepsilon}\label{eq:pp}
\end{eqnarray}

\end_inset


