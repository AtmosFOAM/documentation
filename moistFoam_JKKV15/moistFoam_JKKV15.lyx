\begin_inset Formula 
\begin{equation}
R_{m}=R_{d}+r_{v}R_{v}=R_{d}(1+r_{v}/\varepsilon)
\end{equation}

\end_inset


\begin_inset Formula 
\begin{equation}
c_{pml}=c_{pd}+r_{v}c_{pv}+r_{l}c_{pl}
\end{equation}

\end_inset


\begin_inset Formula 
\begin{equation}
c_{vml}=c_{vd}+r_{v}c_{vv}+r_{l}c_{pl}
\end{equation}

\end_inset


\begin_inset Formula 
\begin{equation}
L_{v}=L_{00}-(T-T_{0})(c_{p\ell}-c_{pv})
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
Condensation:
\begin_inset Formula 
\begin{equation}
S_{\text{cond}}=\frac{\text{FOR}-\text{LIM}+\sqrt{\text{FOR}^{2}+\text{LIM}^{2}}}{\tau_{\text{cond}}}
\end{equation}

\end_inset

where
\begin_inset Formula 
\begin{eqnarray}
\text{FOR} & = & \rho_{v}-p_{vs}/(T\ R_{v})\label{eq:FOR}\\
\text{LIM} & = & \rho_{c}
\end{eqnarray}

\end_inset

where 
\begin_inset Formula $\tau_{\text{cond}}=1\text{s}$
\end_inset

 is the relaxation time-scale from 
\begin_inset CommandInset citation
LatexCommand citet
key "JKKV15"

\end_inset

 and 
\begin_inset Formula $p_{vs}$
\end_inset

 is the saturation vapor pressure which can be found from an approximate
 integration of the Clausius-Clapyeron relation: 
\begin_inset Formula 
\begin{equation}
p_{vs}=p_{vs0}\exp\left(-\frac{L_{v0}}{R_{v}}\left(\frac{1}{T}-\frac{1}{T_{0}}\right)\right)
\end{equation}

\end_inset

where 
\begin_inset Formula $p_{vs0}=611.3\ \text{Pa}$
\end_inset

 is the saturation vapour pressure at 
\begin_inset Formula $T=T_{0}$
\end_inset

.
\end_layout

\begin_layout Standard
Heat sources: 
\begin_inset Formula 
\[
S_{\theta_{\rho}}=-\frac{\theta_{\rho}}{q_{d}}\left(\frac{R_{v}}{R_{m}}-\ln\tilde{\pi}\left[\frac{R_{v}}{R_{m}}+\frac{c_{pl}-c_{pv}}{c_{pml}}\right]-\frac{L_{v}}{c_{pml}T}\right)S_{\text{cond}}
\]

\end_inset


