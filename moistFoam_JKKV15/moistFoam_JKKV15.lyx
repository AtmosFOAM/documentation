\begin_layout Title
Equations Solved in moistFoam
\end_layout

\begin_layout Standard
The flux-form compressible Euler equations from 
\begin_inset CommandInset citation
LatexCommand citet
key "JKKV15"

\end_inset

.
\begin_inset Formula 
\begin{eqnarray}
\frac{\partial\rho\mathbf{u}}{\partial t}+\nabla\cdot\left(\rho\mathbf{u}\mathbf{u}\right) & = & -2\rho\boldsymbol{\Omega}\times\mathbf{u}-\nabla p+\rho\mathbf{g}\\
\frac{\partial\rho}{\partial t}+\nabla\cdot(\rho\mathbf{u}) & = & 0\\
\frac{\partial\rho\theta_{\rho}}{\partial t}+\nabla\cdot\left(\theta_{\rho}\rho\mathbf{u}\right) & = & S_{\theta_{\rho}}\\
\frac{\partial\rho q_{v}}{\partial t}+\nabla\cdot\left(\rho\mathbf{u}q_{v}\right) & = & -S_{\text{cond}}\\
\frac{\partial\rho q_{\ell}}{\partial t}+\nabla\cdot\left(\rho\mathbf{u}q_{\ell}\right) & = & S_{\text{cond}}
\end{eqnarray}

\end_inset

where
\begin_inset Formula 
\begin{equation}
\theta_{\rho}=\theta\left(1+q_{v}\left[\frac{R_{v}}{R_{d}}-1\right]-q_{\ell}\right)=\theta\frac{1+r_{v}/\epsilon}{1+r_{t}}
\end{equation}

\end_inset


\begin_inset Formula 
\begin{equation}
\theta=T(p_{0}/p)^{\kappa_{m}}
\end{equation}

\end_inset


\begin_inset Formula 
\begin{equation}
\theta=T/\pi\ ,\ \ \pi=(p/p_{0})^{\kappa_{m}}\ ,\ \implies p=p_{0}\pi^{1/\kappa_{m}}
\end{equation}

\end_inset


\begin_inset Formula 
\begin{equation}
\implies T=\theta\pi=\theta_{\rho}\pi\frac{1+r_{t}}{1+r_{v}/\varepsilon}=\theta_{\rho}\pi\frac{1}{1-q_{\ell}-q_{v}(1-1/\varepsilon)}
\end{equation}

\end_inset

Eqn of state in terms of 
\begin_inset Formula $p$
\end_inset

 and 
\begin_inset Formula $T$
\end_inset

:
\begin_inset Formula 
\begin{equation}
p=\frac{\rho R_{m}T}{1+r_{t}}=q_{d}\rho R_{m}T
\end{equation}

\end_inset

Eqn of state in terms of 
\begin_inset Formula $\pi$
\end_inset

 and 
\begin_inset Formula $\theta$
\end_inset

:
\begin_inset Formula 
\begin{equation}
\pi^{\frac{1-\kappa_{m}}{\kappa_{m}}}=\frac{\rho R_{d}\theta_{\rho}}{p_{0}}
\end{equation}

\end_inset

Pressure gradient term:
\begin_inset Formula 
\begin{eqnarray}
p & = & p_{0}\pi^{1/\kappa_{m}}\\
\implies\nabla p & = & p_{0}\left\{ \frac{1}{\kappa_{m}}\pi^{\frac{1-\kappa_{m}}{\kappa_{m}}}\nabla\pi+\ln\left(\pi\right)\pi^{1/\kappa_{m}}\nabla\frac{1}{\kappa_{m}}\right\} \\
\implies\nabla p & = & \frac{c_{pml}\rho\theta_{\rho}}{1+r_{v}/\varepsilon}\left\{ \nabla\pi-\frac{\ln\left(\pi\right)\pi}{\kappa_{m}}\nabla\kappa_{m}\right\} 
\end{eqnarray}

\end_inset

Mixing ratios and mass mixing ratios:
\begin_inset Formula 
\begin{align}
q_{v} & =\rho_{v}/\rho & q_{\ell} & =\rho_{\ell}/\rho & q_{d} & =\rho_{d}/\rho\nonumber \\
r_{v} & =\rho_{v}/\rho_{d} & r_{\ell} & =\rho_{\ell}/\rho_{d} & r_{t} & =r_{v}+r_{\ell}\\
\rho & =\rho_{d}+\rho_{v}+\rho_{\ell} & 1 & =q_{d}+q_{v}+q_{\ell} & q_{t} & =q_{v}+q_{\ell}\\
q_{v} & =r_{v}/(1+r_{t}) & q_{\ell} & =r_{\ell}/(1+r_{t}) & q_{t} & =r_{t}/(1+r_{t})\\
r_{v} & =q_{v}/q_{d} & r_{\ell} & =q_{\ell}/q_{d} & q_{d} & =1/(1+r_{t})\\
 &  & 1-q_{\ell} & =(1+r_{v})/(1+r_{t})
\end{align}

\end_inset

Equations relating partial pressures and mixing ratios will also be useful:
\begin_inset Formula 
\begin{eqnarray}
\frac{p_{v}}{p}=\frac{1}{1+\varepsilon/r_{v}} &  & \frac{p_{d}}{p}=\frac{1}{1+r_{v}/\varepsilon}\label{eq:pp}
\end{eqnarray}

\end_inset


\begin_inset Formula 
\begin{eqnarray}
\kappa_{m} & = & R_{m}/c_{pml}
\end{eqnarray}

\end_inset


\end_layout

